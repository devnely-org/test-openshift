    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "${APP_NAME}-php-bimg",
        "labels": {
          "app": "${APP_NAME}",
          "app-service": "${APP_NAME}-${WP_SVC}",
          "build": "${APP_NAME}-${WP_SVC}"

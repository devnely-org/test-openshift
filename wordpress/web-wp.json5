                   // "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin ping" ]
                  },
                  "initialDelaySeconds": 20,
                  "periodSeconds": 20,
                  "failureThreshold": 30
                },
                "startupProbe": {
                  "exec": {
                    "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin status --user=userdb  --host=${HOSTNAME}" ]
                    // "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin ping" ]
                  },
                  "successThreshold": 1,
                  "failureThreshold": 30, 
                  "periodSeconds": 10
                },
                "readinessProbe": {
                  "exec":{
                    // "command": [ "mysqladmin status --user=root --host=$(hostname) --password=$MYSQL_ROOT_PASSWORD" ]
                    "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin status --user=userdb  --host=${HOSTNAME}" ]
                    // "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin ping" ]
                  },
                  "initialDelaySeconds": 20,
                  "periodSeconds": 10,
                  "failureThreshold": 1
                },
                "env": [
                  {
                    "name": "MYSQL_MASTER_SERVICE_NAME",
                    "value": "${APP_NAME}-${MYSQL_SVC}-master"
                  },
                  {
                    "name": "MYSQL_MASTER_USER",
                    "value": "${MYSQL_MASTER_USER}"
                  },
                  {
                    "name": "MYSQL_MASTER_PASSWORD",
                    "value": "${MYSQL_MASTER_PASSWORD}"
                  },
                  {
                    "name": "MYSQL_DATABASE",
                    "value": "${WP_DB_NAME}"
                  },
                  {
                    "name": "MYSQL_USER",
                    "value": "${WP_DB_USER}"
                  },
                  {
                    "name": "MYSQL_PASSWORD",
                    "value": "${WP_DB_PASSWORD}"
                  },
                  {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "${WP_ROOT_DB_PASSWORD}"
                  },
                  {
                    "name": "MYSQL_HOST",
                    "value": "localhost"
                  }
                  // {

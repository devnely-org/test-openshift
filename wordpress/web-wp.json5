{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "Web-Wordpress-Mysql",
    "annotations": {
      "openshift.io/display-name": "Wordpress con mysql8",
      "description": "Creamos la imagen mysql y de wordpress a partir de software collection. Después desplegamos.",
      "tags": "wordpress,mysql",
      "iconClass": "icon-wordpress"
    },
    "labels":{
      "template": "wordpress-mysql"
    }
  },
  "parameters": [
    {
      "name": "APP_NAME",
      "description": "Nombre de la aplicación.",
      "value": "web"
    },
    {
      "name": "WP_SVC",
      "description": "Servicio de wordpress.",
      "value": "wp",
      "required": true
    },
    {
      "name": "MYSQL_SVC",
      "description": "Servicio de mysql.",
      "value": "mysql",
      "required": true
    },
    {
      "name": "WP_VOLUME_SIZE",
      "description": "The password used for the MYSQL_MASTER_USER",
       "value": "8.3Gi"
     },
    {
      "name": "WP_VOLUME_TYPE",
      "description": "The password used for the MYSQL_MASTER_USER",
       "value": "ReadWriteMany"
    },
     {
      "name": "MASTER_VOLUME_SIZE",
      "description": "The password used for the MYSQL_MASTER_USER",
       "value": "8.4Gi"
     },
    {
      "name": "MASTER_VOLUME_TYPE",
      "description": "The password used for the MYSQL_MASTER_USER",
       "value": "ReadWriteMany"
    },
    {
      "name": "SLAVE_VOLUME_SIZE",
      "description": "The password used for the MYSQL_MASTER_USER",
       "value": "8.5Gi"
     },
    {
      "name": "SLAVE_VOLUME_TYPE",
      "description": "The password used for the MYSQL_MASTER_USER",
       "value": "ReadWriteMany"
     },
    {
      "name": "MYSQL_MASTER_USER",
      "description": "The username used for master-slave replication",
      "value": "master"
    },
    {
      "name": "MYSQL_MASTER_PASSWORD",
      "description": "The password used for the MYSQL_MASTER_USER",
      "generate": "expression",
      "value": "passmaster",
      "from": "[a-zA-Z0-9]{12}"
    },
    {
      "name": "SOURCE_REPOSITORY_MYSQL_URL",
      "description": "The URL of the quickstart Git repository.",
      "value": "https://github.com/devnely-org/mysql-container.git",
      "required": true
    },
    {
      "name": "SOURCE_REPOSITORY_MYSQL_TAG",
      "description": "The tagged version of the source repository to use.",
      "value": "dev",
      "required": true
    },
    {
      "name": "SOURCE_REPOSITORY_WP_URL",
      "description": "The URL of the quickstart Git repository.",
      "value": "https://github.com/devnely-org/s2i-php-container.git",
      "required": true
    },
    {
      "name": "SOURCE_REPOSITORY_WP_TAG",
      "description": "The tagged version of the source repository to use.",
      // "value": "#3_add_fpm_wpi_mysqlclient_silo",
      // "value": "#6_fix_assemble_s2i_bin",
      "value": "dev",
      // "value": "#5_change__paths_7.2_Dockerfile",
      "required": true
    },
    {
      "name": "SOURCE_REPOSITORY_WP_PRIVATE_URL",
      "description": "The URL of the quickstart Git repository.",
      "value": "https://github.com/devnely-org/devnely.git",
      "required": true
    },
    {
      "name": "SOURCE_REPOSITORY_WP_PRIVATE_TAG",
      "description": "The tagged version of the source repository to use.",
      "value": "master",
      "required": true
    },
    { "name": "DOCKER_REGISTER",
      "description": "The password for the database user.",
      "value": "docker-registry.default.svc:5000"
    },
    {
      "name": "PROJECT_NAME",
      "description": "Nombre del proyecto openshift para poder acceder a su registro de imágenes de docker.",
      "value": "image-wp"
    },
    //### Todas las variables que empiezan por DB se introducencen desde el contenedor wordpress asi como las que aún no homos usado DB_HOST y DB_TABLE_PREFIX.
    {
      "name": "WP_DB_NAME",
      "description": "Nombre de la base de datos (MYSQL_DATABASE).",
      "value": "wpdb"
    },
    {
      "name": "WP_DB_USER",
      "description": "Usuario de la base de datos (MYSQL_USER).",
      "value": "userdb"
    },
    {
      "name": "WP_DB_PASSWORD",
      "description": "La contraseña del usuario para la base de datos (MYSQL_PASSWORD). Posteriormente la introduciremos como un secreto de kubernetes.",
      "value": "passuserdb"
    },
    {
      "name": "WP_ROOT_DB_PASSWORD",
      "description": "El password del root de msql (MYSQL_ROOT_PASSWORD).  Posteriormente la introduciremos como un secreto de kubernetes.",
      "from": "[a-zA-Z0-9]{12}",
      "value": "passroot"
    },
    {
      "name": "WP_DB_HOST",
      "description": "El nombre del host donde se encuentra la base de datos. Generalmente suele ser el nombre del servicio que expone la base de datos.",
      "value": "web-mysql"
    },
    {
      "name": "WP_DB_PORT_NUMBER",
      "description": "El puerto para comunicarse con la base de datos.",
      "value": "3306"
    },
    {
      "name": "WP_INSTALL_VESION",
      "description": "Versión que se instala en el imagen. En blanco se instalará la \"latest\"",
      "value": "5.2.4"
    },
     {
      "name": "WP_INSTALL_VESION_TAG",
      "description": "Versión que se instala en el imagen. En blanco se instalará la \"latest\"",
      "value": "5x2x4"
    }
  ],
  "objects": [
    {
      "kind": "PersistentVolumeClaim",
      "apiVersion": "v1",
      "metadata": {
        "name": "${APP_NAME}-${WP_SVC}-data",
        "labels": {
          "app": "${APP_NAME}",
          "app-service": "${APP_NAME}-${WP_SVC}",
          "build": "${APP_NAME}-${WP_SVC}",
          "build1": "${APP_NAME}-${WP_SVC}",
          "deploy": "${APP_NAME}-${WP_SVC}"
        }
      },
      "spec": {
        "accessModes": [
          "${WP_VOLUME_TYPE}"
        ],
        "resources": {
          "requests": {
            "storage": "${WP_VOLUME_SIZE}"
          }
        }
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "${APP_NAME}-${MYSQL_SVC}-bimg",
        "labels": {
          "app": "${APP_NAME}",
          "app-service": "${APP_NAME}-${MYSQL_SVC}",
          "build": "${APP_NAME}-${MYSQL_SVC}"
        }
      }
    },
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "${APP_NAME}-${MYSQL_SVC}-bimg",
        "labels": {
          "app": "${APP_NAME}",
          "app-service": "${APP_NAME}-${MYSQL_SVC}",
          "build": "${APP_NAME}-${MYSQL_SVC}"
        }
      },
      "spec": {
        "failedBuildsHistoryLimit": 5,
        "nodeSelector": null,
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "${APP_NAME}-${MYSQL_SVC}-bimg:latest"
          }
        },
        "postCommit": {
        },
        "resources": {
        },
        "triggers": [
          {
            "type": "ConfigChange"
          },
          {
            "type": "ImageChange"
          }
        ],
        "runPolicy": "Serial",
        "source": {
          "type": "Git",
          // "contextDir": "8.0",
          "git": {
            "uri": "${SOURCE_REPOSITORY_MYSQL_URL}",
            "ref": "${SOURCE_REPOSITORY_MYSQL_TAG}"
          }
        },
        "strategy": {
          "type": "Docker",
          "dockerStrategy": {
            "dockerfilePath": "8.0/Dockerfile",
            "forcePull": true,
            "from": {
              "kind": "DockerImage",
              "name": "docker.io/centos/s2i-core-centos7:latest"
            }
            //"env": []
          }
        }
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "${APP_NAME}-${MYSQL_SVC}-img",
        "labels": {
          "app": "${APP_NAME}",
          "app-service": "${APP_NAME}-${MYSQL_SVC}",
          "build": "${APP_NAME}-${MYSQL_SVC}",
          "build1": "${APP_NAME}-${MYSQL_SVC}"
        }
      }
    },
    {
      "apiVersion": "build.openshift.io/v1",
      "kind": "BuildConfig",
      "metadata": {
        "name": "${APP_NAME}-${MYSQL_SVC}-img",
        "labels": {
          "app": "${APP_NAME}",
          "app-service": "${APP_NAME}-${MYSQL_SVC}",
          "build": "${APP_NAME}-${MYSQL_SVC}",
          "build1": "${APP_NAME}-${MYSQL_SVC}"
        }
      },
      "spec": {
        "failedBuildsHistoryLimit": 5,
        "nodeSelector": null,
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "${APP_NAME}-${MYSQL_SVC}-img:latest"
          }
        },
        "postCommit": {
        },
        "resources": {
        },
        "runPolicy": "Serial",
        "source": {
          "contextDir": "test/test-app/mysql-cfg",
          "git": {
            "ref": "${SOURCE_REPOSITORY_MYSQL_TAG}",
            "uri": "${SOURCE_REPOSITORY_MYSQL_URL}"
          },
          "type": "Git"
        },
        "type": "Source",
        "strategy": {
          "sourceStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              // "namespace": "${PROJECT_NAME}",
              "name": "${APP_NAME}-${MYSQL_SVC}-bimg:latest"
            },
            // "env": [ ]
          }
        },
        "triggers": [
          {
            "generic": {
              "secret": "F-ilfroO9PAhjTRSWX67"
            },
            "type": "Generic"
          },
          {
            "type": "ConfigChange"
          },
          {
            "type": "ImageChange"
          }
        ]
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "${APP_NAME}-${MYSQL_SVC}-master",
        "labels": {
          "app": "${APP_NAME}",
          "app-service": "${APP_NAME}-${MYSQL_SVC}",
          "build": "${APP_NAME}-${MYSQL_SVC}",
          "build1": "${APP_NAME}-${MYSQL_SVC}",
          "deploy": "${APP_NAME}-${MYSQL_SVC}-master"
        }
      },
      "spec": {
        "ports": [
          {
            "name": "3306-tcp",
            "protocol": "TCP",
            "port": 3306,
            "targetPort": 3306
          }
        ],
        "selector": {
          "app": "${APP_NAME}",
          "deployconfig":  "${APP_NAME}-${MYSQL_SVC}-master"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "${APP_NAME}-${MYSQL_SVC}-slave",
        "labels": {
          "app": "${APP_NAME}",
          "app-service": "${APP_NAME}-${MYSQL_SVC}",
          "build": "${APP_NAME}-${MYSQL_SVC}",
          "build1": "${APP_NAME}-${MYSQL_SVC}",
          "deploy": "${APP_NAME}-${MYSQL_SVC}-slave"
        }
      },
      "spec": {
        "ports": [
          {
            "name": "3306-tcp",
            "protocol": "TCP",
            "port": 3306,
            "targetPort": 3306
          }
        ],
        "selector": {
          "app": "${APP_NAME}",
          "deployconfig":  "${APP_NAME}-${MYSQL_SVC}-slave"
        }
      } 
    },
    {
      "kind": "PersistentVolumeClaim",
      "apiVersion": "v1",
      "metadata": {
        "name": "${APP_NAME}-master-data",
        "labels": {
          "app": "${APP_NAME}",
          "app-service": "${APP_NAME}-${MYSQL_SVC}",
          "build": "${APP_NAME}-${MYSQL_SVC}",
          "build1": "${APP_NAME}-${MYSQL_SVC}",
          "deploy": "${APP_NAME}-${MYSQL_SVC}"
        }
      },
      "spec": {
        "accessModes": [
          "${MASTER_VOLUME_TYPE}"
        ],
        "resources": {
          "requests": {
            "storage": "${MASTER_VOLUME_SIZE}"
          }
        }
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "${APP_NAME}-${MYSQL_SVC}-master",
        "labels": {
          "app": "${APP_NAME}",
          "app-service": "${APP_NAME}-${MYSQL_SVC}",
          "build": "${APP_NAME}-${MYSQL_SVC}",
          "build1": "${APP_NAME}-${MYSQL_SVC}",
          "deploy": "${APP_NAME}-${MYSQL_SVC}"
        }
      },
      "spec": {
        "strategy": {
          // "type": "Rolling"
          "type": "Recreate"
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": 1,
        "selector": {
          "app": "${APP_NAME}",
          "deployconfig":  "${APP_NAME}-${MYSQL_SVC}-master"
        },
        "template": {
          "metadata": {
            "labels": {
              "app": "${APP_NAME}",
              "deployconfig":  "${APP_NAME}-${MYSQL_SVC}-master"
            }
          },
          "spec": {
            // "serviceAccountName": "default",
            // "securityContext": {
            //   "fsGroup": "0",
            //   "runAsUser": "27",
            //   "runAsGroup": "0"
            // },
            "containers": [
            {
              "name": "${APP_NAME}-${MYSQL_SVC}-master",
                "image": "${DOCKER_REGISTER}/${PROJECT_NAME}/${APP_NAME}-${MYSQL_SVC}-img:latest",
                "args": [
                  "run-mysqld-master"
                ],
                "ports": [
                  {
                    "containerPort": 3306,
                    "protocol": "TCP"
                  }
                ],
                "resources": {
                  "limits": {
                    "memory": "512Mi"
                  }
                },
                "livenessProbe": {
                  "exec":{
                    "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin status --user=userdb --host=${HOSTNAME}" ]
                    // "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin ping" ]
                  },
                  "initialDelaySeconds": 20,
                  "periodSeconds": 20,
                  "failureThreshold": 30
                },
                "startupProbe": {
                  "exec": {
                    "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin status --user=userdb  --host=${HOSTNAME}" ]
                    // "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin ping" ]
                  },
                  "successThreshold": 1,
                  "failureThreshold": 30, 
                  "periodSeconds": 10
                },
                "readinessProbe": {
                  "exec":{
                    "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin status --user=userdb  --host=${HOSTNAME}" ]
                    // "command": [ "mysqladmin status --user=root --host=$(hostname) --password=$MYSQL_ROOT_PASSWORD" ]
                    // "command": [ "/usr/bin/sh", "-i", "-c","MYSQL_PWD=\"$MYSQL_PASSWORD\" mysqladmin status --user=userdb  --host=${HOSTNAME}" ]
                  },
                  "initialDelaySeconds": 20,
                  "periodSeconds": 10,
                  "failureThreshold": 1
                },
                "env": [
                  {
                    "name": "MYSQL_MASTER_USER",
                    "value": "${MYSQL_MASTER_USER}"
                  },
                  {
                    "name": "MYSQL_MASTER_PASSWORD",
                    "value": "${MYSQL_MASTER_PASSWORD}"
                  },
                  {
                    "name": "MYSQL_DATABASE",
                    "value": "${WP_DB_NAME}"
                  },
                  {
                    "name": "MYSQL_USER",
                    "value": "${WP_DB_USER}"
                  },
                  {
                    "name": "MYSQL_PASSWORD",
                    "value": "${WP_DB_PASSWORD}"
                  },
                  {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "${WP_ROOT_DB_PASSWORD}"
                  },
                  {
                    "name": "MYSQL_HOST",
                    "value": "localhost"
                  }
                  // {
                  //   "name": "MYSQL_PORT_NUMBER",
                  //   "value": "${WP_DB_PORT_NUMBER}"
                  //}
                ],
                "volumeMounts": [
                  {
                    "name": "${APP_NAME}-master-data",
                    "mountPath": "/silo"
                  }
                ]
              }
            ],
             "volumes": [
              {
                "name": "${APP_NAME}-master-data",
                "persistentVolumeClaim": {
                  "claimName":  "${APP_NAME}-master-data"
                }
              }
            ]
          }
        }
      }
    },
     {
